package Main;

public class Calc {
	
	public int suma(int val1, int val2) {
			return val1 + val2;
	}
	
	public int resta(int val1, int val2) {
		return val1 - val2;
	}
	
	public int multi(int val1, int val2) {
		return val1 * val2;
	}
	
	public int div(int val1, int val2) {
		return val1 / val2;
	}
	
}
