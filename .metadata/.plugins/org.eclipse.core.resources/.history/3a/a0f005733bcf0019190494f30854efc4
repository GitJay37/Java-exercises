package Main;

public class Dog {
	
	String name, raza;
	int age;
	
	// Parameters
	void setParams(String name, String raza, int age) { // ---> Parameters
		this.name = name;
		this.raza = raza;
		this.age = age;
	}
	
	void eat() {
		System.out.println("El perrito está comiendo :)");
		}
		
	void sleep() {
		System.out.println("El perrito está durmiendo zzz");
	}
	
	void bark() {
		System.out.println("El perrito está ladrando >:P");
	}
}
