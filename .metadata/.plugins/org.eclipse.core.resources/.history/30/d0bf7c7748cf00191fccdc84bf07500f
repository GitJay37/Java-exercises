package Main;

public class User {
	public String username; 
	private String password;
	
	void greet() {
		System.out.println("Hola mi username is: "+this.username);
	}
	
	// Getter method
	
	public String getPass() {
		return this.password;
	}
	
	public void setPass(String password) {
		this.password = password;
	}
}
