package Main;

public class MainClass {

	public static void main(String[] args) {
		// Access modify:
		
		User user = new User();
		User pass = new User();
		
		user.username = "Jay";
		pass.password = "miPass";
		
		
		
		System.out.println(user.username);
		System.out.println(user.password);
		user.greet();
	}

}
